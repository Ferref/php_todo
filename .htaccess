<IfModule mod_rewrite.c>
    RewriteEngine On

    # Ensure register.php can be accessed directly
    RewriteRule ^register.php$ register.php [L]
</IfModule>

# Deny access to .htaccess itself
<Files ".htaccess">
    Require all denied
</Files>

# Allow access to register.php
<Files "register.php">
    Require all granted
</Files>
